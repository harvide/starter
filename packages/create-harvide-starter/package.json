{
  "name": "create-harvide-starter",
  "version": "0.0.5",
  "description": "Create a new Harvide Starter project",
  "author": "Harvide",
  "license": "MIT",
  "type": "module",
  "bin": {
    "create-harvide-starter": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harvide/starter.git"
  },
  "bugs": {
    "url": "https://github.com/harvide/starter/issues"
  },
  "homepage": "https://starter.harvide.com",
  "scripts": {
    "build": "tsup && bash ./copy-template.sh",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.0",
    "vitest": "^1.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.4.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}